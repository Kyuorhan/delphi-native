unit Interfaces.Formularios;

interface

uses
  Vcl.Forms,
  View.Cadastro.Produtos,
  View.Cadastro.Clientes,
  View.Cadastro.Bancos,
  View.Cadastro.Funcionarios;

  type
    iFormularios = interface
      ['{E053B5B1-AD92-46AF-B439-2DCAC6F0129F}']
      function CadastroProdutos(Owner : TForm) : TFrmCadProdutos;
      function CadastroClientes(Owner : TForm) : TFrmCadClientes;
      function CadastroBanco(Owner : TForm) : TFrmCadBancos;
      function CadastroFuncionarios(Owner : TForm) : TFrmCadBancos;
      function CadastroUsuarios(Owner : TForm) : TFrmCadFuncionarios;
    end;

implementation

end.
